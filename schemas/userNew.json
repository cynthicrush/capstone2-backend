{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "title": "Root Schema",
    "type": "object",
    "default": {},
    "required": [
        "username",
        "email",
        "password",
        "first_name",
        "last_name"
    ],
    "properties": {
        "username": {
            "title": "The username Schema",
            "type": "string",
            "default": "",
            "examples": [
                "Cynthicrush"
            ]
        },
        "email": {
            "title": "The email Schema",
            "type": "string",
            "default": "",
            "examples": [
                "cynthia@email.com"
            ]
        },
        "password": {
            "title": "The password Schema",
            "type": "string",
            "default": "",
            "examples": [
                "Password1!"
            ]
        },
        "first_name": {
            "title": "The first_name Schema",
            "type": "string",
            "default": "",
            "examples": [
                "Cynthia"
            ]
        },
        "last_name": {
            "title": "The last_name Schema",
            "type": "string",
            "default": "",
            "examples": [
                "Canada"
            ]
        }
    },
    "examples": [{
        "username": "Cynthicrush",
        "email": "cynthia@email.com",
        "password": "Password1!",
        "first_name": "Cynthia",
        "last_name": "Canada"
    }]
}